{
  "name": "@elizaos/cli",
  "version": "1.0.0-beta.7",
  "description": "elizaOS CLI - Manage your AI agents and plugins",
  "publishConfig": {
    "access": "public",
    "workspaces": {
      "preserveIfPresent": true
    }
  },
  "license": "MIT",
  "author": {
    "name": "elizaOS",
    "url": "https://twitter.com/eliza_OS"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/elizaOS/eliza.git",
    "directory": "packages/cli"
  },
  "files": [
    "dist",
    "templates",
    "drizzle"
  ],
  "workspaces": [
    "packages/*"
  ],
  "keywords": [],
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "bin": {
    "elizaos": "./dist/index.js"
  },
  "scripts": {
    "cli": "bun run build && node dist/index.js",
    "build:core": "cd .. && bun run build:core",
    "build:groq": "cd .. && bun run build:groq",
    "build:sql": "cd .. && turbo run build --filter=@elizaos/plugin-sql --no-cache",
    "build:dbml": "cd .. && turbo run build --filter=@elizaos/plugin-dbml --no-cache",
    "build:all": "cd .. && turbo run build --filter=@elizaos/plugin-sql --filter=@elizaos/plugin-groq --filter=@elizaos/core --no-cache",
    "train": "bun run dist/index.js train",
    "pivot": "bun run dist/index.js pivot",
    "train-debug": "bun run --inspect-brk=192.168.1.90:6299 dist/index.js",
    "build": "tsup && bun run src/scripts/copy-client-dist.ts && bun run src/scripts/copy-templates.ts && bun run src/scripts/copy-drizzle.ts",
    "lint": "prettier --write ./src",
    "format": "prettier --write ./src",
    "format:check": "prettier --check ./src",
    "clean": "rm -rf dist .turbo node_modules .turbo-tsconfig.json tsconfig.tsbuildinfo"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/parser": "^7.22.6",
    "@elizaos/core": "^1.0.0-beta.7",
    "@elizaos/plugin-anthropic": "^1.0.0-beta.7",
    "@elizaos/plugin-openai": "^1.0.0-beta.7",
    "@elizaos/plugin-sql": "workspace:*",
    "@elizaos/plugin-dbml": "workspace:*",
    "@types/babel__core": "^7.20.1",
    "@types/diff": "^5.0.3",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash": "^4.17.7",
    "@types/node": "^20.11.27",
    "@types/prompts": "^2.4.2",
    "axios": "^1.7.9",
    "commander": "^10.0.0",
    "diff": "^5.1.0",
    "dotenv": "^16.4.5",
    "execa": "^7.0.0",
    "fs-extra": "^11.1.0",
    "glob": "^11.0.0",
    "https-proxy-agent": "^6.2.0",
    "prettier": "3.5.3",
    "prompts": "^2.4.2",
    "recast": "^0.23.2",
    "rimraf": "6.0.1",
    "tsconfig-paths": "^4.2.0",
    "tsup": "8.4.0",
    "tsx": "^4.19.2",
    "type-fest": "^3.8.0",
    "typescript": "5.8.2",
    "vitest": "^1.3.1",
    "yoctocolors": "^2.1.1",
    "@hapi/shot": "^6.0.1",
    "@types/hapi": "^18.0.14"
  },
  "gitHead": "9834bbd06128356b44b091f022fc2a2d024a875e",
  "dependencies": {
    "@electric-sql/pglite": "^0.2.17",
    "@elizaos/plugin-discord": "workspace:*",
    "@elizaos/plugin-groq": "workspace:*",
    "@elizaos/plugin-local-ai": "workspace:*",
    "@hapi/shot": "^6.0.1",
    "@types/hapi": "^18.0.14",
    "csv-parse": "^5.6.0",
    "ffmpeg-static": "^5.2.0",
    "pino-caller": "^3.4.0",
    "prism-media": "^1.3.5",
    "socket.io": "^4.8.1",
    "zod": "3.24.2"
  }
}
