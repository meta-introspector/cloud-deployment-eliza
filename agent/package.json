{
    "name": "@elizaos/agent",
    "version": "0.25.6-alpha.1",
    "main": "src/index.ts",
    "type": "module",
    "scripts": {
        "start_prof": "node --heap-prof --cpu-prof  --prof --inspect-brk=0.0.0.0:9229 --require mytracing.js --loader ts-node/esm src/index.ts",
        "start_debug": "node --inspect-brk=0.0.0.0:9229 --require mytracing.js --loader ts-node/esm src/index.ts",
        "start": "node --loader ts-node/esm src/index.ts",
        "dev": "node --loader ts-node/esm src/index.ts",
        "check-types": "tsc --noEmit",
        "test": "jest"
    },
    "nodemonConfig": {
        "watch": [
            "src",
            "../core/dist"
        ],
        "ext": "ts,json",
        "exec": "node --enable-source-maps --loader ts-node/esm src/index.ts"
    },
    "dependencies": {
        "@confio/ics23": "^0.6.8",
        "@elizaos-plugins/client-discord": "workspace:*",
        "@elizaos-plugins/client-twitter": "workspace:*",
        "@elizaos-plugins/plugin-speech-tts": "workspace:*",
        "@elizaos-plugins/plugin-twitter": "workspace:*",
        "@elizaos/client-direct": "workspace:*",
        "@elizaos/core": "workspace:*",
        "@elizaos/plugin-bootstrap": "workspace:*",
        "@hapi/shot": "^6.0.1",
        "@opentelemetry/api": "^1.9.0",
        "@opentelemetry/auto-instrumentations-node": "^0.39.4",
        "@opentelemetry/sdk-node": "^0.45.1",
        "@roamhq/wrtc": "^0.8.0",
        "@sinclair/typebox": "^0.32.35",
        "@tsconfig/next": "^2.0.3",
        "@types/hapi": "^18.0.14",
        "agent-twitter-client": "workspace:*",
        "child_process": "^1.0.2",
        "created": "^0.6.1",
        "events": "^3.3.0",
        "fs": "0.0.1-security",
        "headers-polyfill": "^3.3.0",
        "json-stable-stringify": "^1.2.1",
        "otpauth": "^9.3.6",
        "path": "^0.12.7",
        "readline": "1.3.0",
        "set-cookie-parser": "^2.7.1",
        "tough-cookie": "^4.1.4",
        "twitter-api-v2": "^1.20.2",
        "value": "link:@sinclair/typebox/value",
        "ws": "8.18.0",
        "yargs": "17.7.2"
    },
    "devDependencies": {
        "@types/jest": "^29.5.14",
        "jest": "^29.7.0",
        "ts-jest": "^29.2.6",
        "ts-node": "10.9.2",
        "tsup": "8.3.5"
    }
}
